{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "conan": {
    "enabled": true
  },
  "hostRules": [{
    "matchHost": "https://conan.jfreeman.dev",
    "hostType": "conan",
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["cupcake"],
      "matchDatasources": ["conan"],
      "registryUrls": ["https://conan.jfreeman.dev"]
    },
    {
      "matchDatasources": ["conan"],
      "registryUrls": ["https://center.conan.io"]
    }
  ]
}